      <Command Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">@echo off
setlocal enabledelayedexpansion enableextensions

goto voidMain

:fn_GetDeviceId
set file=Config.ini
set area=Device
set key=Id
set currarea=
for /f "usebackq delims=" %%a in ("!file!") do (
    set ln=%%a
    if "x!ln:~0,1!"=="x[" (
        set currarea=!ln!
    ) else (
        for /f "tokens=1,2 delims==" %%b in ("!ln!") do (
            set currkey=%%b
            set currval=%%c
            if "x!area!"=="x!currarea!" if "x!key!"=="x!currkey!" (
                echo "Selected device id is !currval!. To change device id, open Config.ini"
            )
        )
    )
)
goto returnMain

:voidMain
echo.
echo ==========================================
echo = Compiling over Particle Cloud          =
echo ==========================================
del *.bin


set firstFile=

for %%x in (*.cpp) do set firstFile=!firstFile! "%%x"
 
for %%x in (*.h) do set firstFile=!firstFile! "%%x"

cmd /c particle compile photon%firstFile%
echo.
echo ==========================================
echo = Flashing over cloud                    =
echo ==========================================
goto fn_GetDeviceId
:returnMain
particle flash %currval% photon~1.bin
echo ==========================================
echo.
echo = FLASHING DONE. PLEASE WAIT WHILE YOU PARTICLE PHOTON REBOOTS. =</Command>
